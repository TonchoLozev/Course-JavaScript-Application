<section id="contacts">
    <div class="container">
        {{>header}}
        <header>&#9993; Contact Book</header>
        <div id="book" class="box-large">
            <div id="list">
                <h1>Contacts</h1>
                <div class="content">
                    {{>contact}}
                </div>
            </div>
            <div id="details">
                <h1>Details</h1>
                <div class="content">
                    {{>details}}
                </div>
            </div>
        </div>
        {{>footer}}
    </div>
</section>