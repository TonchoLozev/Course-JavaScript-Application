{{> header}}
<div class="content">
    {{> navigation}}
    <section id="viewMyPosts">
        <div class="post post-content">
            <h1>Your Posts</h1>
        </div>
        <div class="posts">
          {{#each posts}}
              {{> post}}
          {{else}}
              <p>You don't have any posts</p>
          {{/each}}
        </div>
    </section>
</div>
{{> footer}}